# about:config

browser.urlbar.scotchBonnet.enableOverride -> false (remove search engine icon from url bar)

browser.vpn.promo.enabled -> false (remove vpn promo from private window)

browser.privatebrowsing.vpnpromourl -> clear value (remove vpn promo from private window)

ui.key.menuAccessKeyFocuses -> false (disable press 'alt' toggles menu options)

screen-api.transition-duration.enter -> 0 0 (disable fullscreen transition on enter)

screen-api.transition-duration.leave -> 0 0 (disable fullscreen transition on leave)

full-screen-api.warning.timeout -> 0 (remove fullscreen warning)


